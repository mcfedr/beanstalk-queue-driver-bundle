{
    "name": "mcfedr/beanstalk-queue-driver-bundle",
    "type": "symfony-bundle",
    "description": "A bundle for managing job queues",
    "keywords": ["queue", "job", "schedule", "task", "beanstalkd"],
    "autoload": {
        "psr-4": { "Mcfedr\\BeanstalkQueueDriverBundle\\": "src/Mcfedr/BeanstalkQueueDriverBundle/"}
    },
    "autoload-dev": {
        "psr-4": { "Mcfedr\\BeanstalkQueueDriverBundle\\" : "tests/Mcfedr/BeanstalkQueueDriverBundle/" }
    },
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "^2.8|^3.0",
        "mcfedr/queue-manager-bundle": "^2.2",
        "pda/pheanstalk": "^3.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.1",
        "symfony/monolog-bundle": "^2.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fred Cox",
            "email": "mcfedr@gmail.com"
        }
    ]
}
